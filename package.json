{
  "name": "rx-http-kit",
  "version": "1.0.8",
  "description": "HTTP Client Development Kit based by rxjs",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "files": [
    "lib",
    "types",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "format": "prettier --write --cache .",
    "lint": "eslint --cache packages/*/{src,types,__tests__}/**",
    "rollup": "tsc && rollup -c",
    "test:compile": "tsc -p ./tsconfig-test.json",
    "test:server": "npm run test:compile && node test/dist/server.js",
    "test:mocha": "mocha 'test/dist/*.spec.js'",
    "types": "tsc -p ./tsconfig.types.json",
    "prepublishOnly": "npm run rollup && npm run types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MQpeng/rx-http-client.git"
  },
  "keywords": [
    "HTTP",
    "Http-Kit",
    "Http-development-kit",
    "Rxjs",
    "Vue",
    "Angular",
    "Axios",
    "Request",
    "AJAX",
    "Typescript"
  ],
  "author": "tonyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MQpeng/rx-http-client/issues"
  },
  "homepage": "https://github.com/MQpeng/rx-http-client#readme",
  "packageManager": "pnpm@7.3.0",
  "publishConfig": {
    "directory": "dist"
  },
  "pnpm": {
    "overrides": {},
    "packageExtensions": {}
  },
  "dependencies": {
    "rxjs": "^7.5.5",
    "xhr2": "^0.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "esbuild": "^0.14.47",
    "eslint": "^8.18.0",
    "mocha": "^10.0.0",
    "pnpm": "^7.4.0",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "tsx": "^3.6.0",
    "typescript": "^4.7.4"
  },
  "optionalDependencies": {
    "axios": "^0.27.2"
  },
  "peerDependencies": {
    "axios": "^0.27.2"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": true
    }
  },
  "types": "types/index.d.ts"
}
