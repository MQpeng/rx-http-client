{
  "name": "rx-http-kit",
  "version": "1.0.0",
  "description": "HTTP Client Development Kit based by rxjs",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write --cache .",
    "lint": "eslint --cache packages/*/{src,types,__tests__}/** playground/**/__tests__/**/*.ts scripts/**",
    "typecheck": "tsc -p scripts --noEmit && tsc -p playground --noEmit",
    "build": "pnpm -r --filter=./packages/* run build",
    "dev": "pnpm -r --parallel --filter=./packages/* run dev",
    "release": "tsx scripts/release.ts",
    "ci-publish": "tsx scripts/publishCI.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MQpeng/rx-http-client.git"
  },
  "keywords": [
    "HTTP",
    "Http-Kit",
    "Http-development-kit",
    "Rxjs",
    "Vue",
    "Angular",
    "Axios",
    "Request",
    "AJAX",
    "Typescript"
  ],
  "author": "tonyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MQpeng/rx-http-client/issues"
  },
  "homepage": "https://github.com/MQpeng/rx-http-client#readme",
  "packageManager": "pnpm@7.3.0",
  "publishConfig": {
    "directory": "dist"
  },
  "pnpm": {
    "overrides": {},
    "packageExtensions": {}
  },
  "dependencies": {
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "esbuild": "^0.14.47",
    "eslint": "^8.18.0",
    "pnpm": "^7.4.0",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "tsx": "^3.6.0",
    "typescript": "^4.7.4"
  },
  "optionalDependencies": {
    "axios": "^0.27.2",
    "xhr2": "^0.2.1"
  }
}
